<div class='tutorials-container'>
  {{#if data}}
    {{#each data}}
      <div
        id='{{_id}}'
        class='tutorial-progress'
        style='background: conic-gradient( #8b949e 0%,  #8b949e {{progress}}%, #1f242c {{progress}}%, #1f242c 100%);'
      >
        <div class='tutorial'>
          <div class='title-container'>
            <h3 class='tutorial-title'>{{name}}</h3>
          </div>
        </div>
      </div>
    {{/each}}
  {{/if}}
