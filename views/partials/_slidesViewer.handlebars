<div id='slides-viewer' class='slides-viewer'>
  <div class='slides-header'>
    <div class='slides-progress'>
      <div class='prograss-frag'>
        <div class='progress-bar' style='background-color: #8b949e'></div>
        <div class='progress-disc' style='background-color: #8b949e'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar' style='background-color: #8b949e'></div>
        <div class='progress-disc' style='background-color: #8b949e'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar' style='background-color: #8b949e'></div>
        <div class='progress-disc' style='background-color: #8b949e'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar'></div>
        <div class='progress-disc'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar'></div>
        <div class='progress-disc'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar'></div>
        <div class='progress-disc'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar'></div>
        <div class='progress-disc'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar'></div>
        <div class='progress-disc'></div>
      </div>
      <div class='prograss-frag'>
        <div class='progress-bar'></div>
        <div class='progress-disc'></div>
      </div>
    </div>
    <div class='add-slide'>
      <input
        id='inheader-slide-id'
        class='inheader-slide-id'
        type='text'
        placeholder='existing slide id'
        {{! value='627cdc26ceaccba121754f9d' }}
      />
      <button class='add-slide-btn' id='add-slide-btn'>
        <i class='fa fa-plus'></i>
      </button>
    </div>
  </div>
  <div class='slides-body'>
    <input
      id='incanvas-slide-id'
      class='incanvas-slide-id'
      type='text'
      readonly
      value='627cdc26ceaccba121754f9d'
    />
    <canvas id='canvas' class='canvas'></canvas>
    <div id='slide-img' class='slide-img' alt=''></div>
  </div>
  <div class='slide-controls'>
    <button class='slide-controls-btn' id='slide-show-notes-btn'>notes</button>
    <button class='slide-controls-btn' id='slide-add-notes-btn'>add notes</button>
    <div class='audio-player'>
      {{! <i class='fa fa-microphone'></i> }}
      {{! <i class='fa fa-square'></i> }}
      {{! <i class='fa fa-circle' style='color: red ;'></i> }}
      <div id='play-btn'>
        <i class='fa fa-play'></i>
      </div>
      <input type='range' class='timeline' max='100' value='0' />
      {{! <i class='fa fa-repeat'></i> }}
      <i class='fa fa-volume-up'></i>
      {{! <i class='fa fa-volume-off'></i> }}

      <audio
        src='https://www.learningcontainer.com/wp-content/uploads/2020/02/Kalimba.mp3'
      ></audio>
    </div>
    <div id='expand-btn'>
      <div class='fa fa-expand'></div>
    </div>
    <div id='compress-btn'>
      <div class='fa fa-compress'></div>
    </div>
    <button class='slide-controls-btn' id='slide-submit-btn'>submit</button>
  </div>
</div>